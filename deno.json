{
  "tasks": {
    "check": "deno fmt --check . && deno lint . && deno check",
    "dev": "vite",
    "build": "vite build",
    "start": "deno serve -A _fresh/server.js",
    "update": "deno run -A -r jsr:@fresh/update ."
  },
  "imports": {
    "@/": "./",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@^1.0.7",
    "@preact/signals": "npm:@preact/signals@^2.3.2",
    "@sqlite.org/sqlite-wasm": "npm:@sqlite.org/sqlite-wasm@^3.50.4-build1",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.12",
    "basecoat-css": "npm:basecoat-css@^0.3.2",
    "fresh": "jsr:@fresh/core@^2.1.4",
    "lucide-preact": "npm:lucide-preact@^0.552.0",
    "preact": "npm:preact@^10.27.2",
    "vite": "npm:vite@^7.1.3",
    "tailwindcss": "npm:tailwindcss@^4.1.10"
  },
  "nodeModulesDir": "auto",
  "unstable": ["raw-imports"],
  "lint": {
    "rules": {
      "tags": ["fresh", "recommended"]
    }
  },
  "exclude": ["**/_fresh/*"],
  "compilerOptions": {
    "lib": ["dom", "dom.asynciterable", "dom.iterable", "deno.ns"],
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": ["vite/client"]
  },
  "deploy": {
    "org": "bbcwqx",
    "app": "ts-lyrics"
  }
}
